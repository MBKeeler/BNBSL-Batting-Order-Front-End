<legend>Select a Season</legend>
{{#each seasons as |season|}}

  <table style="width:425px" cellspacing="25">
    <thead>
        <th align="left">Year</th>
        <th align="left">Team</th>
        <th align="left">Program</th>
    </thead>
    {{#if season}}
    <tbody>
        {{!-- {{#seasons}} --}}
        {{!-- <tr bgcolor="#cd9966" data-id="{{season.id}}"> --}}
            {{!-- <td align="center">{{season.id}}</td> --}}
            <td align="left">{{season.year}}</td>
            <td align="left">{{season.team}}</td>
            <td align="left">{{season.program}}</td>
            <tdalign="left"><button id="selectSeasonBttn" value="{{season.id}}" name='select' type='click'>Select</button></td>
        </tr>
        {{!-- {{/seasons}} --}}
  </table>
{{else}}
    <h4>There are no seasons on record for this user. Return to the Tools aand create a new season<h4>
{{/if}}

{{/each}}
